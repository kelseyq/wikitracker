{
  "name": "WikiTracker",
  "version": "1.0",
  "manifest_version": 2,
  "description": "following you down the wiki-hole",
  "content_scripts": [
    {
      "matches": ["http://*.wikipedia.org/*"],
      "js" : ["jquery.min.js", "json2-min.js", "underscore-1.3.3-min.js", "backbone-0.9.2-min.js", "2.5.3-crypto-sha1-hmac.js", "stackmob-js-0.5.5-min.js", "StackMobInit.js", "extension.js"]
    }
  ],
  "permissions": [
    "http://*.stackmob.com/*",
    "https://*.stackmob.com/*"
  ]
}
